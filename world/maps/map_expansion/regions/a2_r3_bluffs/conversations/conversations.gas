	[text*]
	{
		choice = shop;
		sample = s_v_a2_ilorn_01_c;
		screen_text = "Azunai praise you, outlander! This is indeed my amulet; an amulet given to me by my late wife. I was traveling to Illicor, our great city, to trade my goods there, when I was ambushed by several jungle slingers. I fled here to escape, but must have dropped the amulet. I am grateful for your aid.  I am Ilorn, a tradesman, and I am Utraean. Interested in a new sword, or perhaps a spell? Look no further, for I have them all!";
	  	f scroll_rate = 20.0000;
	}
}

